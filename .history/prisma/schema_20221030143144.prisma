// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  length  Int    @id
  title   String
  content String
  date    String
}

model Friend {
  id         Int    @id @default(autoincrement())
  name       String
  relation   String
  created_at String
}
